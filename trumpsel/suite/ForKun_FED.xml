<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="FED" parallel="false">

<parameter name="scenarioName" value="ForKun_FED.xlsx"/>
<parameter name="indexAppServers" value="8"/>
<parameter name="indexDBServers" value="8"/>
<parameter name="browser" value="firefox"/>
<parameter name="browser_version" value="49"/>
 <test name="CheckValidation"  enabled="false">
  	<parameter name="scenarioName" value="ForKun_FEDExportValidation.xlsx"/>
    <classes>
      <class name="com.lombardrisk.testcase.CheckValue">
      <methods>
      <include name="checkValidation"/>
      </methods>
      </class>
    </classes>
</test>
 <test name="DataScheduleExport" enabled="false">
  	<parameter name="scenarioSheet" value="FEDDataSchedule"/>
    <classes>
      <class name="com.lombardrisk.testcase.ExportToRegulator">
      <methods>
      <include name="checkInForm"/>
      </methods>
      </class>
    </classes>
</test>


 <test name="Retrieve" enabled="false">
  	<parameter name="scenarioSheet" value="FEDRetrieve"/>
    <classes>
      <class name="com.lombardrisk.testcase.RetrieveForm">
      <methods>
      <include name="RetrieveForm"/>
      </methods>
      </class>
    </classes>
</test>

<test name="ExportToRegulator"  enabled="false">
  	<parameter name="scenarioSheet" value="FEDExportToRegulator"/>
    <classes>
      <class name="com.lombardrisk.testcase.ExportToRegulator">
      <methods>
      <include name="checkInForm"/>
      <include name="checkInDashBoard"/>
      </methods>
      </class>
    </classes>
</test>



<test name="ExportToPDF" enabled="false">
  	<parameter name="scenarioName" value="ForKun_FEDExportToPDF.xlsx"/>
    <classes>
      <class name="com.lombardrisk.testcase.CheckValue">
	  <methods>
      <include name="checkExportToPDF"/>
      </methods>
      </class>
    </classes>
</test>


<test name="ExportCSV" >
  	<parameter name="scenarioName" value="ForKun_FEDExportToCSV.xlsx"/>
    <classes>
      <class name="com.lombardrisk.testcase.CheckValue">
	  <methods>
      <include name="checkExportToCSV"/>
      </methods>
      </class>
    </classes>
</test>
<test name="ExportExcel">
  	<parameter name="scenarioName" value="ForKun_FEDExportToExcel.xlsx"/>
    <classes>
      <class name="com.lombardrisk.testcase.CheckValue">
	  <methods>
      <exclude name="checkExportToExcelApplyScale"/>
      <include name="checkExportToExcelNoScale"/>
      </methods>
      </class>
    </classes>
</test>

</suite>
