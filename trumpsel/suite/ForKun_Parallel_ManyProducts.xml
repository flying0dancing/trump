<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ABC" parallel="tests" thread-count="3">


<parameter name="indexAppServers" value="9"/>
<parameter name="indexDBServers" value="9"/>
<parameter name="browser" value="chrome"/>
<!--<parameter name="browser_version" value="35"/>-->

<test name="HKMAExportCSV"  enabled="true">
	<parameter name="indexAppServers" value="5"/>
	<parameter name="indexDBServers" value="5"/>
  	<parameter name="scenarioSheet" value="ExportToCSV"/>
  	<parameter name="scenarioName" value="ForKun_Smoke_HKMA.xlsx"/>
    <classes>
      <class name="com.lombardrisk.testcase.CheckValue">
	  <methods>
      <include name="checkExportToCSV"/>
      </methods>
      </class>
    </classes>
</test>

<test name="MASExportCSV"  enabled="true">
	<parameter name="indexAppServers" value="5"/>
	<parameter name="indexDBServers" value="5"/>
  	<parameter name="scenarioSheet" value="ExportToCSV"/>
  	<parameter name="scenarioName" value="ForKun_Smoke_MAS.xlsx"/>
    <classes>
      <class name="com.lombardrisk.testcase.CheckValue">
	  <methods>
      <include name="checkExportToCSV"/>
      </methods>
      </class>
    </classes>
</test>

<test name="ECRExportExcel" enabled="true">
    <parameter name="indexAppServers" value="0"/>
	<parameter name="indexDBServers" value="0"/>
  	<parameter name="scenarioSheet" value="ExportToEXCEL"/>
  	<parameter name="scenarioName" value="ForKun_Smoke_ECR.xlsx"/>
    <classes>
      <class name="com.lombardrisk.testcase.CheckValue">
	  <methods>
      <include name="checkExportToExcelApplyScale"/>
      <exclude name="checkExportToExcelNoScale"/>
      </methods>
      </class>
    </classes>
</test>

</suite>
